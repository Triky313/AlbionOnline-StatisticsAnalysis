                        <Style.Triggers>
                            <DataTrigger Binding="{Binding Type}" Value="{x:Static enumerations:DungeonEventObjectType.BookChest}">
                                <Setter Property="Visibility" Value="Visible"/>
                            </DataTrigger>
                            <DataTrigger Binding="{Binding Type}" Value="{x:Static enumerations:DungeonEventObjectType.Chest}">
                                <Setter Property="Visibility" Value="Visible"/>
