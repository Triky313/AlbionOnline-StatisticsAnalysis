                    <Style TargetType="{x:Type Grid}">
                        <Setter Property="Visibility" Value="Hidden" />
                        <Style.Triggers>
                            <DataTrigger Binding="{Binding Type}" Value="{x:Static enumerations:DungeonEventObjectType.CombatShrine}">
                                <Setter Property="Visibility" Value="Visible"/>
