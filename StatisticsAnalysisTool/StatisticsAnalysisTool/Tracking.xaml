            <TextBlock Text="{Binding Quantity, StringFormat='{}{0}x '}" Foreground="{StaticResource Solid.Color.Text.Normal}"/>
            <Image Source="{Binding Item.Icon}" Width="25" Height="25" Stretch="Fill" />
            <TextBlock Text="{Binding Item.LocalizedName, StringFormat='{}{0} '}" Foreground="{StaticResource Solid.Color.Text.Normal}"/>

            <TextBlock Text="{Binding FromTranslation, StringFormat='{}{0} '}" Foreground="{StaticResource Solid.Color.Text.Normal}"/>
            <TextBlock Text="{Binding LootedPlayer, StringFormat='{}{0} '}" Foreground="{StaticResource SolidColorBrush.Blue.3}"/>
        </StackPanel>
    </DataTemplate>

