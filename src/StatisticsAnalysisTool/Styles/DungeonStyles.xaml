    <Style x:Key="DungeonNotificationFragment.BestFame" TargetType="Label">
        <Style.Triggers>
            <DataTrigger Binding="{Binding IsBestFame}" Value="true">
                <Setter Property="Foreground" Value="{StaticResource SolidColorBrush.Accent.Yellow.1}" />
                <Setter Property="Effect">
                    <Setter.Value>
                        <DropShadowEffect BlurRadius="10" ShadowDepth="0" Opacity="1" Color="{StaticResource Color.Accent.Yellow.2}"/>
                    </Setter.Value>
                </Setter>
            </DataTrigger>
            <DataTrigger Binding="{Binding IsBestFame}" Value="false">
                <Setter Property="Foreground" Value="{StaticResource SolidColorBrush.Text.1}" />
            </DataTrigger>
        </Style.Triggers>
    </Style>

